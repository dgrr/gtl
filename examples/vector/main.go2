package main

import (
	"sort"
	"fmt"
	"math/rand"

	"github.com/dgrr/gtl"
)

func main() {
	var vec gtl.Vec[int]

	for i := 0; i < 20; i++ {
		vec.Append(128)
		vec.Append(
			rand.Intn(100))
	}

	vec.DelFn(func(it gtl.Iterator[int]) bool {
		return it.V() == 128
	})

	sort.Slice(vec, func(i, j int) bool {
		return vec[i] < vec[j]
	})

	for vec.Len() != 0 {
		fmt.Println(
			vec.PopFront())
	}

	vec.Push(-20)

	fmt.Println("last value", vec.PopFront())
}
