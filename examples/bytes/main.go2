package main

import (
	"fmt"

	"github.com/dgrr/gtl"
)

func main() {
	data := []byte("Hello world\r\n1234")

	b := gtl.Bytes{}
	fmt.Println("len", b.Len())

	b.Resize(len(data))

	fmt.Println("len after resize", b.Len())

	b.CopyFrom(data)

	fmt.Println(b.Index('0'))
	fmt.Println(
		b[:b.Index('\r')].String())
}
