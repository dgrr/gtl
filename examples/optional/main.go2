package main

import (
	"fmt"

	"github.com/dgrr/gtl"
)

func myFunc(shouldHave bool) (op gtl.Optional[int]) {
	if shouldHave {
		op.Set(20)
	}
	
	return op
}

func main() {
	v := myFunc(true)
	if v.Ok() {
		fmt.Println("has value", v.V())
	} else {
		v.Set(20)
	}

	v.Drop()
	fmt.Println("after drop:", v.Ok(), v.V())
}
