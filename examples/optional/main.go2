package main

import (
	"fmt"

	"github.com/dgrr/gtl"
)

func myFunc(shouldHave bool) (o gtl.Optional[int]) {
	if shouldHave {
		o.Set(20)
	}
	
	return o
}

func main() {
	v := myFunc(true)
	if v.Has() {
		fmt.Println("has value", v.V())
	} else {
		v.Set(20)
	}

	v.Drop()
	fmt.Println("after drop:", v.Has(), v.V())
}
